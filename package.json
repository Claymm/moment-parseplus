{
  "name": "moment-parseplus",
  "version": "1.0.0",
  "description": "Date parsing plugin for momentjs",
  "main": "parseplus.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter spec",
    "test-browser": "open test/index.html",
    "coverage": "node ./node_modules/istanbul/lib/cli.js cover node_modules/.bin/_mocha -- -- test/**/*.spec.js",
    "view-coverage": "open ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensnyder/moparse.git"
  },
  "keywords": [
    "moment",
    "momentjs",
    "date",
    "parse",
    "parsing"
  ],
  "author": "Ken Snyder <kendsnyder@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kensnyder/moment-parseplus/issues"
  },
  "homepage": "https://github.com/kensnyder/moment-parseplus#readme",
  "dependencies": {
    "moment": "^2.14.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "requirejs": "^2.2.0"
  }
}
